{
	"manifest_version": 3,
	"name": "Panopto Keyboard Speed Controller",
	"version": "0.0",
	"description": "Adjust video playback speed with '+' and '-' keys.",
	"permissions": [],
	"content_scripts": [
		{
			"matches": [
				"*:\/\/*.panopto.eu/*"
			],
			"js": [
				"speed_changer.js"
			]
		}
	],
	"icons": {
		"16": "icons/icon-16.png",
		"48": "icons/icon-48.png",
		"128": "icons/icon-128.png"
	}
}